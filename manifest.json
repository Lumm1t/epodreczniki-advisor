{
  "name": "Epodreczniki.pl advisor",
  "description": "Show correct answers on epodreczniki.pl",
  "version": "1.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.epodreczniki.pl/*"],
      "css": ["epodrecznikiadvisor.min.css"],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "assets/icons/epodrecznikiadvisor16.png",
    "32": "assets/icons/epodrecznikiadvisor32.png",
    "48": "assets/icons/epodrecznikiadvisor48.png",
    "128": "assets/icons/epodrecznikiadvisor128.png"
  }
}
